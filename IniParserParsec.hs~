import Text.ParserCombinators.Parsec

parse = do sections <- many section
           eof
           return sections
           
section = do title

title = do char '['
           nom <- many anyChar
           string "]\n"
           return nom

variable = do n <- name
              char '='
              v <- value
              char '\n'
              return (n, v)

name = do 
              